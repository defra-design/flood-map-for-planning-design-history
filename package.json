    "rimraf": "^6.0.0",
    "sass": "^1.80.5"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "standard": "^17.1.0",
    "webshot-node": "^0.18.2"
  },
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "type": "module"
}
